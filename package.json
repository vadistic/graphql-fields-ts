{
  "name": "@vadistic/graphql-fields-ts",
  "version": "2.0.3",
  "description": "TypeScript fork of `graphql-fields`. Turns GraphQLResolveInfo into a map of the requested fields",
  "repository": "https://github.com/vadistic/graphql-fields-ts",
  "author": "Jakub Wadas <vadistic@gmail.com>",
  "license": "MIT",
  "keywords": [
    "graphql",
    "graphql-js",
    "graphqlresolveinfo",
    "fields",
    "schema",
    "ast",
    "typescript",
    "fork"
  ],
  "main": "dist/index.js",
  "module": "dist/graphql-fields-ts.esm.js",
  "types": "dist/index.d.ts",
  "source": "src/index.ts",
  "files": [
    "src/*",
    "dist/*"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "test": "tsdx test",
    "lint": "tsdx lint .",
    "build": "tsdx build --target node --tsconfig ./tsconfig.build.json",
    "prepublishOnly": "yarn clena && yarn build"
  },
  "bugs": {
    "url": "https://github.com/robrichard/graphql-fields/issues"
  },
  "homepage": "https://github.com/robrichard/graphql-fields#readme",
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "@types/node": "^13.11.0",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "eslint": "^6.8.0",
    "eslint-config-react-app": "^5.2.1",
    "graphql": "^15.0.0",
    "jest": "^25.2.7",
    "tsdx": "^0.13.1",
    "typescript": "^3.8.3"
  }
}
